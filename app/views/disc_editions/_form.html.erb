<%= form_for([@disc, @disc.disc_editions.build]) do |f| %>
  <div class="field">
    <%= f.label :plastic %><br />
    <%= f.text_field :plastic %>
  </div>
  <div class="field">
    <%= f.label :price %><br />
    <%= f.text_field :price %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>